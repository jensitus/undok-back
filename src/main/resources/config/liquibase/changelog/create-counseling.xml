<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="create_counseling_table" author="jensitus">

        <createTable tableName="counseling">
            <column name="id" type="UUID">
                <constraints primaryKey="true"/>
            </column>
            <column name="counselingStatus" type="varchar(255)"/>
            <column name="entryDate" type="date"/>
            <column name="concern" type="varchar(255)" />
            <column name="concernCategory" type="varchar(255)" />
            <column name="activity" type="varchar(255)" />
            <column name="activityCategory" type="varchar(255)" />
            <column name="registeredBy" type="varchar(255)" />
        </createTable>

    </changeSet>



</databaseChangeLog>